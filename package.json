{
  "name": "nasaPhotoOfDay",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "react-scripts": "0.9.5"
  },
  "dependencies": {
    "axios": "0.15.3",
    "react": "15.4.2",
    "react-dom": "15.4.2"
  },
  "scripts": {
    "build-css": "node-sass public/ -o public/",
    "watch-css": "npm run build-css && node-sass public/ -o public/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
